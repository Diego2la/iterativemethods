.lab
  text-align center

.matrix
  input
    border none
    width 40px
    font-size 14px
    padding 2px
    &:hover
      background-color #eee
    &:focus
      background-color #ccf
    &:not(:focus)
      text-align right
  table
    border-collapse collapse
  td
    border 1px solid #999
    padding 0
  tr:first-child td, td:first-child
    background-color #ccc
    padding 1px 3px
    font-weight bold;
    text-align center